@charset "utf-8";

html,body,dl,dt,dd,pre,p,blockquote,fieldset,input {
	background: none repeat scroll 0 0 rgba(36, 36, 36, 1);
	border: 0 none;
	font-size: 100%;
	margin: 0;
	outline: 0 none;
	padding: 0;
	vertical-align: baseline;
}

body {
	background-color: #242424;
	font: 12px/20px "Lucida Grande", Arial, Sans-serif;
	margin: 0;
	padding: 0;
	text-align: left;
	font-weight: 400;
	font-style: normal;
	color: #333333;
	text-transform: none;
	text-decoration: none;
	letter-spacing: normal;
	word-spacing: 0;
	line-height: 13px;
	text-align: left;
	vertical-align: baseline;
	direction: ltr;
	height: 100%;
	max-height: 100%;
}

a {
	color: #fff;
	cursor: pointer;
	outline: medium none;
	text-decoration: underline;
}

header {
	background: none repeat scroll 0 0 #1D1D1D;
	font-size: 10px;
	height: 90px;
	letter-spacing: 0.5px;
	padding-bottom: 5px;
	position: relative;
	width: 100%;
	z-index: 8000;
}

article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section
	{
	display: block;
}

.container {
	margin: 0 auto;
	padding: 0 14px;
	width: 1024px;
}

header .container {
	position: relative;
	width: 1024px;
	z-index: 2;
	background: none repeat scroll 0 0 #1D1D1D;
}

#fsLogo {
	background: url("../images/logo.png") no-repeat scroll center center
		#1D1D1D;
	height: 70px;
	position: absolute;
	top: 0;
	width: 300px;
	left: 10px;
	padding: 5px 15px;
}

.mainContainer {
	background-color: #242424;
	position: relative;
	clear: both;
	height: 455px;
	overflow: hidden;
	padding-bottom: 5px;
	width: 1024px;
	margin: 30px auto 0 auto;
	border-bottom: 1px #999999 solid;
	-webkit-box-shadow: 0px px 3px #999999;
	-moz-box-shadow: 0px 3px 3px #999999;
	box-shadow: 0px 3px 0px #999999;
}

.footer {
	text-align: center;
}

#footerLinks {
	width: 880px;
	text-align: center;
	font-size: 10px;
}

#footerLinks ul,li {
	list-style: none;
	color: #999999;
	display: inline-block;
	padding: 0px 8px;
}

#footerLinks #footerLinksList {
	float: right;
	margin: 0;
	padding: 0;
}

#footerLinks #footerLinksList li {
	color: #DADADA;
	list-style: none outside none;
	display: block;
	margin: 0 0 0 3px;
	padding: 0 0 0 3px;
}

footer a,.footer a {
	color: #999999;
	text-decoration: none;
}

footer a:hover,.footer a:hover {
	text-decoration: underline;
}

.login {
	background: none repeat scroll 0 0 rgba(0, 0, 0, 0);
	height: 50px;
	right: 0;
	padding: 20px 15px;
	position: absolute;
	top: 0;
	z-index: 2;
}

.login input[type="text"] {
	background: none repeat scroll 0 0 rgba(0, 0, 0, 0);
	border: 1px solid rgba(255, 255, 255, 0.6);
	border-radius: 2px;
	color: #FFFFFF;
	font-size: 14px;
	font-weight: 400;
	margin-right: 5px;
	padding: 4px;
	width: 150px;
	display: inline-block;
}

.login input[type="password"] {
	background: none repeat scroll 0 0 rgba(0, 0, 0, 0);
	border: 1px solid rgba(255, 255, 255, 0.6);
	border-radius: 2px;
	color: #FFFFFF;
	font-size: 14px;
	font-weight: 400;
	margin-top: 10px;
	margin-right: 5px;
	padding: 4px;
	width: 150px;
	cursor: pointer;
	display: inline-block;
}

.login button[type="button"] {
	background: -moz-linear-gradient(center top, #1872D9 20%, #135AAC 100%)
		repeat scroll 0 0 rgba(0, 0, 0, 0);
	background: -webkit-linear-gradient(to top, #1872D9 20%, #135AAC 100%)
		repeat scroll 0 0 rgba(0, 0, 0, 0);
	background: linear-gradient(to top, rgba(24, 114, 217, 1) 20%,
		rgba(19, 90, 172, 1) 100%) repeat scroll 0 0 rgba(0, 0, 0, 0);
	border: 1px solid #265890;
	box-shadow: 0 1px 0 0 #5E9DE4 inset;
	color: #FFFFFF;
	opacity: 1;
	text-shadow: 1px 1px 1px #000000;
	padding: 4px;
	font-size: 14px;
	font-weight: 400;
	margin-top: 10px;
	width: 80px;
	cursor: pointer;
	display: inline-block;
}

